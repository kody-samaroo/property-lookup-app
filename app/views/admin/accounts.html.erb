<h2> Accounts </h2>
<% @accounts.each do |account| %>
    <div class="row">
        <div class="col-sm-3">
            <%= account.first_name + " " + account.last_name %>
        </div>
        <div class="col-sm-3">
            <%= account.email %>
        </div>
        <div class="col-sm-3">
            <%= account.created_at.strftime("%d %m %Y") %>
        </div>
        <div class="col-sm-3">
            <%= link_to "View Profile", profile_path(account)  %>
        </div>
    </div>
<% end %>