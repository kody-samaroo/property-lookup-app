<% @properties.each do |property| %>
    <div class="col-4">
        <%= link_to(property_thumbnail(property), property_path(property)) %>
        <h3 class="h5 mt-4"><%= property.name %></h3>
        <p><%= property.address %></p>
        <div class="amenities">      
            <span><i data-feather="users"></i></span>
            <span></span>
            <span></span>
        </div>
    </div>
<% end %>